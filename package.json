{
	"name": "cloudflare-images-client",
	"version": "1.1.4",
	"description": "Complete wrapper around cloudflare images api",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.cjs",
	"types": "./dist/index.d.ts",
	"files": ["dist"],
	"scripts": {
		"dev": "tsc src/index.ts",
		"build": "tsup",
		"test": "vitest run",
		"test:watch": "vitest",
		"coverage": "vitest run --coverage",
		"format": "npx @biomejs/biome format --write ./src/*"
	},
	"devDependencies": {
		"@biomejs/biome": "1.7.0",
